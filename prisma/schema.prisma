generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgres"
  url      = env("DATABASE_URL")
}

model tarefas {
  idtarefas  Int    @id @default(autoincrement())
  tarefa     String @db.VarChar(45)
  prioridade String @db.VarChar(45)
}

model concluidos {
  idconcluidos Int    @id @default(autoincrement())
  tarefa       String @db.VarChar(45)
  prioridade   String @db.VarChar(45)
}
